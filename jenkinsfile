pipeline
{
agent any
stages
{
stage('Continous-download')
{
steps
{
git 'https://github.com/Kollurdilip98/New-developer-code.git'
}
}
stage('Continous-build')
{
steps
{
sh 'mvn package'
}
}
stage('Continous-deployment')
{
steps
{
deploy adapters: [tomcat9(credentialsId: 'dilip', path: '', url: 'http://3.91.46.215:8080')], contextPath: 'dilip', war: '**/*.war'
}
}
stage('Continous-test')
{
steps
{
git 'https://github.com/Kollurdilip98/Testing-Code.git'
sh 'java -jar /var/lib/jenkins/workspace/Test/testing.jar'
}
}
stage('Continous-Delivery')
{
steps
{
deploy adapters: [tomcat9(credentialsId: 'dilip', path: '', url: 'http://18.234.46.111:8080')], contextPath: 'deepak', war: '**/*.war'
}
}
}
}
