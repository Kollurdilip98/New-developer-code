node('built-in')
{
stage('Continous-Download')
{
git 'https://github.com/Kollurdilip98/New-developer-code.git'
}
stage('Continous-Build')
{
sh 'mvn package'
}
stage('Continous-Deployment')
{
deploy adapters: [tomcat9(credentialsId: 'dilip', path: '', url: 'http://3.91.46.215:8080/')], contextPath: 'bubby', war: '**/*.war'
}
stage('Continous-Testing')
{
git 'https://github.com/Kollurdilip98/Testing-Code.git'
sh 'java -jar /var/lib/jenkins/workspace/dilip/testing.jar'
}
stage('Continous-Delivery')
{
deploy adapters: [tomcat9(credentialsId: 'dilip', path: '', url: 'http://18.234.46.111:8080/')], contextPath: 'mahesh', war: '**/*.war'
}
}
